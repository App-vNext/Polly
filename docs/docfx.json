{
  "metadata": [
    {
      "src": [
        {
          "cwd": "../src",
          "files": [
            "**/**.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**",
            "Polly/**",
            "v7/**"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "EnumSortOrder": "alphabetic",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "globalMetadata": {
      "_appLogoPath": "icon.png",
      "_appFaviconPath": "icon.png"
    },
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/readme.md"
        ]
      },
      {
        "files": [
          "**.md",
          "**.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": "**/*.{cs}",
        "src": "src",
        "dest": "api"
      }
    ],
    "resource": [
      {
        "files": [
          "icon.png"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
