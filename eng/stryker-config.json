{
  "stryker-config": {
    "reporters": [
      "dots",
      "json",
      "html",
      "markdown"
    ],
    "ignore-methods": [
      "*Exception.ctor",
      "AddError",
      "ConfigureAwait",
      "Dispose",
      "LogError",
      "Debug.Assert",
      "LogInformation"
    ],
    "ignore-mutations": [
      "block",
      "statement"
    ],
    "concurrency": 4,
    "configuration": "Debug",
    "language-version": "Preview",
    "_comment": "TODO Update to net11.0 when Stryker supports it",
    "target-framework": "net10.0",
    "thresholds": {
      "high": 100,
      "low": 100
    }
  }
}
